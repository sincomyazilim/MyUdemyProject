@model List<ResultAboutDto> 
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

@*About Index*@

<table class="table tabcontent-border">
    <tr>
        <th>#</th>
        <th>Başlık 1</th>
        <th>Başlık 2</th>
        <th>İçerik</th>
        <th>Güncelle</th>
        
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.AboutId</th>
            <td>@item.Title1</td>
            <td>@item.Title2</td>
            <td>@item.Content</td>
            <td><a href="/AdminAbout/UpdateAbout/@item.AboutId" class="btn btn-outline-primary">Güncelle</a></td>
            
        </tr>
    }
</table>



@*v93*@