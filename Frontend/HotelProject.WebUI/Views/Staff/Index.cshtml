@model List<StaffViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table tabcontent-border">
    <tr>
        <th>#</th>
        <th>Personel Adı</th>
        <th>Persoenel Ünvanı</th>
        <th>sil</th>
        <th>Güncelle</th>
        <th>Detaylar</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.StaffId</th>
            <td>@item.Name</td>
            <td>@item.Title</td>
            <td><a href="/Staff/DeleteStaff/@item.StaffId" class="btn btn-outline-danger">Sil</a></td>
            <td><a href="/Staff/UpdateStaff/@item.StaffId" class="btn btn-outline-primary">Güncelle</a></td>
            <td><a href="#" class="btn btn-outline-info">Detaylar</a></td>
        </tr>
    }
</table>

<a href="/Staff/AddStaff/" class="btn btn-outline-success">Yeni personel Ekle</a>

