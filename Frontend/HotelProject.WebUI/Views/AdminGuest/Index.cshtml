@model List<ResultGuestDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
    int count = 0;
}

<table class="table tabcontent-border">
    <tr>
       
        
        <th>#</th>
        <th>Ad </th>
        <th>soyad</th>
        <th>Şehir</th>
        <th>sil</th>
        <th>Güncelle</th>

    </tr>
    @foreach (var item in Model)
    {
        count++;
        <tr>
            <th>@count</th>
            <td>@item.Name</td>
            <td>@item.Surname</td>
            <td>@item.City</td>
            <td><a href="/AdminGuest/DeleteGuest/@item.Id" class="btn btn-outline-danger">Sil</a></td>
            <td><a href="/AdminGuest/UpdateGuest/@item.Id" class="btn btn-outline-primary">Güncelle</a></td>

        </tr>
    }
</table>

<a href="/AdminGuest/AddGuest/" class="btn btn-outline-success">Ekle</a>

