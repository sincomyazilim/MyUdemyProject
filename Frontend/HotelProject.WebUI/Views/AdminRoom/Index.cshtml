@model List<ResultRoomDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
    int count = 0;
}

<table class="table tabcontent-border">
    <tr>
        <th>#</th>
        <th>Oda Numarası</th>
        <th>Başlık</th>
        <th>sil</th>
        <th>Güncelle</th>
     
    </tr>
    @foreach (var item in Model)
    {
        count++;
        <tr>
            <th>@count</th>
            <td>@item.RoomNumber</td>
            <td>@item.Title</td>
            <td><a href="/AdminRoom/DeleteRoom/@item.RoomId" class="btn btn-outline-danger">Sil</a></td>
            <td><a href="/AdminRoom/UpdateRoom/@item.RoomId" class="btn btn-outline-primary">Güncelle</a></td>
          
        </tr>
    }
</table>

<a href="/AdminRoom/AddRoom/" class="btn btn-outline-success">Ekle</a>

